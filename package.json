{
  "name": "typescript-starter-node",
  "version": "1.0.0",
  "description": "A starter package for typescript projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check-engines": "check-engines",
    "staged-mods-ok": "git-mods --staged_ok",
    "no-mods": "git-mods",
    "copy": "cd src && find -E . -regex '.*\\.(json|css)' -type f -exec rsync -R {} ../build \\; && cd ..",
    "clean": "rimraf build coverage",
    "clean:dist": "rimraf dist",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "dev": "env-cmd development-app webpack-dev-server --config ./webpack.config.js --inline --hot",
    "build": "npm run build:dev-app",
    "build:dev-app": "npm-run-batch",
    "build:dev-lib": "npm-run-batch",
    "build:dist-app": "npm-run-batch",
    "build:dist-lib": "npm-run-batch",
    "webpack:dev-app": "env-cmd development-app webpack --display-error-details  --config ./webpack.config.js --color --progress",
    "webpack:dev-lib": "env-cmd development-lib webpack --display-error-details  --config ./webpack.config.js --color --progress",
    "webpack:dist-app": "env-cmd production-app webpack --display-error-details  --config ./webpack.config.js --color --progress --mangle --compress warnings=false",
    "webpack:dist-lib": "env-cmd production-lib webpack --display-error-details  --config ./webpack.config.js --color --progress",
    "build:watch": "tsc -w",
    "css": "node ./node_modules/typed-css-modules/lib/cli.js src",
    "npm-sh": "npm-sh",
    "secure": "nsp check",
    "check": "npm-check -u",
    "test-with-cover": "istanbul cover ./node_modules/.bin/_mocha",
    "test-without-cover": "mocha",
    "remap-coverage:dist": "node ./scripts/remapped-coverage.js --force_min_cover true --output ./coverage/typescript",
    "remap-coverage": "node ./scripts/remapped-coverage.js --force_min_cover=false --output ./coverage/typescript",
    "coverage": "istanbul check-coverage ./coverage/typescript/coverage.json",
    "test": "run-batch",
    "test:dist": "run-batch"
  },
  "run-batch": {
    "build:dev-app": [
      "check-engines",
      "clean",
      "lint",
      "css",
      "webpack:dev-app"
    ],
    "build:dev-lib": [
      "check-engines",
      "clean",
      "lint",
      "css",
      "webpack:dev-lib"
    ],
    "build:dist-app": [
      "check-engines",
      "clean:dist",
      "lint",
      "css",
      "webpack:dist-app"
    ],
    "build:dist-lib": [
      "check-engines",
      "clean:dist",
      "lint",
      "css",
      "webpack:dist-lib"
    ],
    "test:dist": [
      "check-engines",
      "test-with-cover",
      "remap-coverage:dist"
    ],
    "test": [
      "check-engines",
      "test-with-cover",
      "remap-coverage"
    ]
  },
  "pre-commit": [
    "build:dist-lib",
    "staged-mods-ok"
  ],
  "pre-commit:SKIPPED-FOR-NOW": [
    "test",
    "build:dist",
    "staged-mods-ok"
  ],
  "pre-push": [
    "build:dist-lib",
    "check",
    "secure",
    "no-mods"
  ],
  "pre-push:SKIPPED-FOR-NOW": [
    "coverage",
    "check",
    "secure",
    "test:dist",
    "no-mods"
  ],
  "keywords": [
    "TypeScript",
    "starter",
    "node"
  ],
  "author": "Shishir Ramam",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.30",
    "@types/enzyme": "^2.5.39",
    "@types/expect": "^1.13.31",
    "@types/mocha": "^2.2.34",
    "@types/node": "^6.0.52",
    "@types/react": "^0.14.52",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/redux-devtools": "^3.0.36",
    "@types/redux-devtools-dock-monitor": "^1.1.29",
    "@types/redux-devtools-log-monitor": "^1.0.29",
    "@types/reselect": "^2.0.27",
    "@types/sinon": "^1.16.32",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "check-engines": "^1.2.0",
    "commander": "^2.9.0",
    "cross-env": "^3.1.3",
    "env-cmd": "^4.0.0",
    "enzyme": "^2.4.1",
    "git-mods": "^1.0.1",
    "graceful-fs": "^4.1.5",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "istanbul": "^0.4.4",
    "jscrambler-loader": "^2.0.2",
    "licensecheck": "^1.0.0",
    "mocha": "^3.0.1",
    "npm-check": "^5.2.3",
    "npm-run-batch": "0.0.4",
    "npm-sh": "^1.0.1",
    "nsp": "^2.6.1",
    "nsp-formatter-checkstyle": "^1.0.1",
    "nsp-formatter-remarkup": "^1.0.2",
    "pre-commit": "^1.1.3",
    "pre-push": "^0.1.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-chart-monitor": "^1.4.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-extension": "^1.0.0",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.7",
    "remap-istanbul": "^0.8.4",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0",
    "shelljs-nodecli": "^0.1.1",
    "sinon": "^1.17.5",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^1.3.3",
    "ts-node": "^1.2.2",
    "tslint": "^4.1.1",
    "tslint-loader": "^3.3.0",
    "typed-css-modules": "^0.1.12",
    "typescript": "^2.1.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-vendor-chunk-plugin": "^1.0.0"
  },
  "dependencies": {
    "css-loader": "^0.26.1",
    "postcss-cssnext": "^2.7.0",
    "postcss-focus": "^1.0.0",
    "postcss-reporter": "^3.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.13.0",
    "reselect": "^2.5.3",
    "style-loader": "^0.13.1",
    "window-or-global": "^1.0.1"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
